#include<stdio.h>
int sum(int i,int j,int k){
    printf("%d\n",i+j+k);
}
int sub(int i,int j,int k){
    printf("%d\n",i+j-k);
}
int mul(int i,int j,int k){
    printf("%d\n",i*j*k);
}

int main()
{
    int a,b,c;
    printf("enter the numbers");
    scanf("%d %d %d",&a,&b,&c);
    sum(a,b,c);
    sub (a,b,c);
    mul(a,b,c);
}

