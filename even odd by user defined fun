#include<stdio.h>
int even(int i, int j)
{
    int k;
    for(k=i;k<=j;k++){
        if (k%2==0)
        printf("\n%d",k);
    }
    
}
    int odd (int i,int j)
    {
        int k;
        for (k=i;k<=j;k++)
        if(k%2!=0)
        printf("\n%d",k);
}
int main()
{
    int a,b;
    printf("enter the range");
    scanf("%d%d",&a,&b);
    printf("even");
    even(a,b);
    printf("\nodd");
    odd(a,b);
}

